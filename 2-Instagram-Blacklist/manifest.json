{
    "manifest_version" : 2,
    "name" : "bhy_soft İnstagram Kara Liste",
    "description" : "Sizin takip ettiğiniz kişilerin sizi takip edip etmediğinizi hızlı ve kolay bir şekilde görüntüleyin.",
    "version" : "1.0",
    "background" : {
        "scripts" : ["./bg.js"]
    },
    "browser_action" : {
        "default_popup" : "index.html"

    },
    "permissions" : [
        "tabs",
        "https://www.instagram.com/*"
    ],
    "icons":{
        "16" : "icons/icon16.png",
        "32" : "icons/icon32.png",
        "48" : "icons/icon48.png",
        "96" : "icons/icon96.png"
    }
}